        {
            "Name": "HasTranslationGuide",
            "Value": "false"
        },
        {
            "Name": "HasTranslationGuide",
            "Value": "true",
            "When": {
                "HasWalletItem": "DwarvishTranslationGuide"
            }
        },
        {
            "Name": "HasSomeCrabsEnabled",
            "Value": "true"
        },
        {
            "Name": "HasSomeCrabsEnabled",
            "Value": "false",
            "When": {
                "DisabledAnimals |contains=CopperCrab": "true",
                "DisabledAnimals |contains=IronCrab": "true",
                "DisabledAnimals |contains=GoldCrab": "true",
